@model IEnumerable<Magistral40.Models.Prices>

@{
    ViewBag.Title = "Цены на нашу продукцию";
    ViewBag.KeyWord = "цена на песок, цена на щебень, цена на нерудный материалы, прайс-лист";
    ViewBag.Description = "Цены на продукцию компании ООО &quot;Магистраль&quot;";

    Layout = "~/Views/Shared/_LayoutT.cshtml";
    var mod = Model.GroupBy(x => x.Group_Name);

}

<h1>Стоимость нашей продукции</h1>
<div class="magContext">
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IE)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Price)
            </th>
            
        </tr>
        @foreach (var item in mod)
        {
            <tr>
                <td colspan="3" style="text-align:center;background-color:#D76366;color:#fff;">  @Html.DisplayFor(modelItem => item.Key)</td>
            </tr>
            foreach (var t in item)
            {
                <tr>
                    <td>

                        @Html.DisplayFor(modelItem => t.Name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => t.IE)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => t.Price)
                    </td>
                </tr>
            }

        }


    </table>
</div>