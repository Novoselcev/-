@model IEnumerable<Magistral40.Models.Prices>
@{
  
    Layout = null;
}
    
<div style="display:inline-block ;">
 @foreach (var t in Model)
     {
        <div class="prodPrice">
         <img src= '~/Content/images/Product/izvest510.jpg' />
          <div class="ProdPrText"> @Html.DisplayFor(modelItem => t.Group_Name) <br> @Html.DisplayFor(modelItem => t.Name) </div>
            <span  class="btn btn-primary btn-lg">@Html.DisplayFor(modelItem => t.Price) руб./@Html.DisplayFor(modelItem => t.IE)</span>
        </div>
       }
</div>